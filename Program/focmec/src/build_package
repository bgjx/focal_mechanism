#!/bin/csh -f
echo The default for FCOMP assumes you are using gfortran
echo Edit this file if you are using a different compiler
setenv FCMP "gfortran -fbackslash"
#setenv FCMP "g77"
#setenv FCMP "f77"
cd ../lib
\rm *
cd ../bin
\rm -f dsretc fm2focmec focmec_prep focmec focplt freesurf hypo2foc \
radiat ratio_prep ratplt sgftops sgfswap symdemo ttimes vwbeetle
cd ../fmsub
make
if ($status != 0) exit
cd ../iaspei-tau/src
make
if ($status != 0) exit
mv libiasp.a ../../lib
\rm -f ../../bin/ttimes
mv ../bin/ttimes ../../bin
cd ../build-tables
./build-tables
if ($status != 0) exit
mv *.hed *.tbl ../../lib
\rm -R stuff iasp91.term ak135.term nocrust.term
cd ../src
make clean
cd ../../src
make
